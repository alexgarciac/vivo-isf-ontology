    <!-- http://purl.obolibrary.org/obo/ERO_0000599 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ERO_0000599">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dietary intervention</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ERO_0000347"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An intervention that involves dietary and nutritional management. The concept does not include vitamin or mineral supplements, for which &quot;drug intervention&quot; may be used.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MeSH qualifier id: Q000178.   Definition excerpted from MeSH.</rdfs:comment>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON: Melanie Wilson</obo:IAO_0000119>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PERSON: Melanie Wilson</obo:IAO_0000117>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To treat some illnesses or conditions, such as diabetes, a dietary intervention may be used.</obo:IAO_0000112>
        <obo:IAO_0000111 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dietary intervention</obo:IAO_0000111>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000120"/>
