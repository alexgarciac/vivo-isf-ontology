    <!-- http://xmlns.com/foaf/0.1/Agent -->

    <owl:Class rdf:about="&foaf;Agent">
        <rdfs:label xml:lang="en-us">Agent</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&vivo;freetextKeyword"/>
