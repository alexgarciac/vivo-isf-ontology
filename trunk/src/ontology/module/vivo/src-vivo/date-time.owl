    <owl:Ontology rdf:about="http://purl.obolibrary.org/obo/arg/date-time.owl"/>
	
	    <!-- http://vivoweb.org/ontology/core#DateTimeInterval -->

    <owl:Class rdf:about="http://vivoweb.org/ontology/core#DateTimeInterval">
        <rdfs:label xml:lang="en-us">Date/Time Interval</rdfs:label>
        <ero:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a specific period or duration, defined by (optional) start and end date/times.</ero:IAO_0000115>
    </owl:Class>
	    <!-- http://vivoweb.org/ontology/core#DateTimeValue -->

    <owl:Class rdf:about="http://vivoweb.org/ontology/core#DateTimeValue">
        <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1/span#TemporalInstant"/>
    </owl:Class>
	    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label"/>
		
